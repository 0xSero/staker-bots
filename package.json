{
  "name": "staker-bots",
  "version": "1.0.0",
  "description": "Staker bot implementation",
  "type": "module",
  "scripts": {
    "start": "COMPONENTS=all tsx src/index.ts",
    "dev": "tsx watch src/index.ts",
    "start:database": "COMPONENTS=database tsx src/index.ts",
    "start:monitor": "COMPONENTS=monitor tsx src/index.ts",
    "start:executor": "COMPONENTS=executor tsx src/index.ts",
    "start:profitability": "COMPONENTS=profitability tsx src/index.ts",
    "start:deposit-scanner": "COMPONENTS=deposit-scanner tsx src/index.ts",
    "start:reward-checker": "COMPONENTS=reward-checker tsx src/index.ts",
    "dev:database": "COMPONENTS=database tsx watch src/index.ts",
    "dev:monitor": "COMPONENTS=monitor tsx watch src/index.ts",
    "dev:executor": "COMPONENTS=executor tsx watch src/index.ts",
    "dev:profitability": "COMPONENTS=profitability tsx watch src/index.ts",
    "dev:deposit-scanner": "COMPONENTS=deposit-scanner tsx watch src/index.ts",
    "dev:reward-checker": "COMPONENTS=reward-checker tsx watch src/index.ts",
    "start:core": "COMPONENTS=database,monitor,executor tsx src/index.ts",
    "start:rewards": "COMPONENTS=database,deposit-scanner,reward-checker,profitability,executor tsx src/index.ts",
    "start:all": "COMPONENTS=all tsx src/index.ts",
    "dev:core": "COMPONENTS=database,monitor,executor tsx watch src/index.ts",
    "dev:rewards": "COMPONENTS=database,deposit-scanner,reward-checker,profitability,executor tsx watch src/index.ts",
    "dev:all": "COMPONENTS=all tsx watch src/index.ts",
    "dashboard": "BULL_BOARD=true tsx src/index.ts",
    "dashboard:dev": "BULL_BOARD=true tsx watch src/index.ts",
    "start:full": "COMPONENTS=all BULL_BOARD=true tsx src/index.ts",
    "dev:full": "COMPONENTS=all BULL_BOARD=true tsx watch src/index.ts",
    "prod": "NODE_ENV=production EXECUTOR_TYPE=defender COMPONENTS=all tsx src/index.ts",
    "prod:core": "NODE_ENV=production EXECUTOR_TYPE=defender COMPONENTS=database,monitor,executor tsx src/index.ts",
    "prod:rewards": "NODE_ENV=production EXECUTOR_TYPE=defender COMPONENTS=database,deposit-scanner,reward-checker,profitability,executor tsx src/index.ts",
    "test": "src/tests/run-tests.sh",
    "test:monitor": "src/tests/run-test.sh monitor",
    "test:profitability": "src/tests/run-test.sh profitability",
    "test:executor": "src/tests/run-test.sh executor",
    "test:full": "src/tests/run-test.sh full",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "build": "tsc",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@bull-board/api": "^4.6.4",
    "@bull-board/express": "^4.6.4",
    "@openzeppelin/defender-relay-client": "^1.54.6",
    "@openzeppelin/defender-sdk": "^2.3.0",
    "@supabase/supabase-js": "^2.48.1",
    "axios": "^1.7.9",
    "bull-board": "^2.1.3",
    "bullmq": "^4.6.0",
    "dotenv": "^16.4.5",
    "ethers": "^6.13.5",
    "events": "^3.3.0",
    "express": "^5.1.0",
    "ioredis": "^5.6.0",
    "testcontainers": "^10.23.0",
    "uuid": "^11.0.5",
    "vitest": "^3.1.1",
    "web3": "4.7.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.1",
    "@types/ioredis": "^5.0.0",
    "@types/node": "^22.14.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.2.5",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "zod": "^3.24.2"
  }
}
